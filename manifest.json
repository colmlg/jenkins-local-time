{
  "name": "Jenkins Local Timezone",
  "version": "1.0",
  "description": "Converts Jenkins server time to the user's local timezone",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["moment.min.js", "content.js"]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}