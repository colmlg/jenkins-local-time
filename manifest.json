{
  "name": "Jenkins Local Timezone",
  "version": "1.0",
  "description": "Converts Jenkins server time to the user's local timezone",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["moment.min.js", "content.js"]
    }
  ],
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
   "128": "images/icon128.png" 
  },
  "manifest_version": 2
}